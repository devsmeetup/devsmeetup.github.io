{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  },
  "exclude": [".cache", "node_modules", "dist"]
}
